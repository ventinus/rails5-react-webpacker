{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%",
          "uglify": true
        },
        "useBuiltIns": true
      }
    ],
    {
      "plugins": [
        ["relay", {"schema": "schema.json"}]
      ]
    },
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "relay",
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ]
  ]
}
